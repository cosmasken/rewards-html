    "dependencies": {
        "@tailwindcss/line-clamp": "^0.4.0",
        "concurrently": "^7.1.0",
        "cross-env": "^7.0.3",
        "cssnano": "^5.1.7",
        "live-server": "^1.2.1",
        "node-sass": "^7.0.1",
        "postcss-cli": "^9.1.0"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.0",
