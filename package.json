{
    "name": "rewards-landing-template",
    "version": "1.0.0",
    "description": "Rewards  Landing Template",
    "main": "index.js",
    "scripts": {
        "serve": "cross-env NODE_ENV=development concurrently \"postcss src/input.css -o src/build/output.css --watch\"  \"live-server ./src\"",
        "development": "cross-env NODE_ENV=development postcss src/input.css -o src/build/output.css",
        "production": "cross-env NODE_ENV=production postcss src/input.css -o src/build/output.css",
        "start": "npm run serve"
    },
    "repository": {
        "type": "git",
        "url": "git+https://cosmasaruba@bitbucket.org/bitfactor/rewards-landing-template.git"
    },
